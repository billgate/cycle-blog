    "babel-tape-runner": "^2.0.1",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "browserify-hmr": "^0.3.1",
    "eslint": "^2.5.3",
    "eslint-config-cycle": "^3.2.0",
    "faucet": "0.0.1",

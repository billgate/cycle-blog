{
  "name": "cycle-blog",
  "version": "1.0.0",
  "description": "Simple blog app built with Cycle and Rx",
  "main": "app.js",
  "scripts": {
    "lint": "eslint src/",
    "test-server": "NODE_ENV=test babel-tape-runner 'src/server/**/*.spec.js' | faucet",
    "babel-node": "NODE_ENV=development babel-node 'src/server/index.js'",
    "dev-server": "nodemon --watch src/server --exec npm run babel-node",
    "watch-test-server": "nodemon --watch src/server --exec npm run -s test-server"
  },
  "keywords": [
    "blog",
    "cycle",
    "universal",
    "js",
    "reactive"
  ],
  "author": "Michal Vanko",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "*",
    "babel-eslint": "^5.0.0-beta6",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-tape-runner": "^2.0.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "browserify-hmr": "^0.3.1",
    "eslint": "^1.10.3",
    "eslint-config-cycle": "^3.2.0",
    "faucet": "0.0.1",
    "less": "^2.6.0",
    "supertest": "^1.1.0",
    "tape": "^4.4.0",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "@cycle/core": "^6.0.2",
    "@cycle/dom": "^9.0.2",
    "@cycle/history": "*",
    "@cycle/http": "^8.0.0",
    "dejavu-fonts-ttf": "^2.35.5",
    "koa": "^1.1.2",
    "koa-logger": "^1.3.0",
    "koa-mount": "^1.3.0",
    "koa-route": "^2.4.2",
    "koa-router": "^5.3.0",
    "koa-watchify": "^1.1.1",
    "mongoose": "^4.4.2",
    "normalize.css": "^3.0.3",
    "ramda": "^0.19.1",
    "rx": "^4.0.7",
    "switch-path": "^1.1.3",
    "winston": "^2.1.1"
  }
}
